---
import Paper from "../components/Paper.astro";
import paper_json from "~/database/json/papers.json"
// posts.reverse();

---
<div class="flex flex-col gap-5">
  {
    Object.values(paper_json).map((data, idx) => (
      <>
        <ul>
          <Paper
            title={data.title}
            me={data.me}
            author={data.author}
            publishedDate={(data.pubDate?.[0] ?? "") as string}
            confName={data.confName}
            category={data.tags}
            postscript={data.postscript}
            paperUrl={data?.paperURL}
            codeUrl={data?.codeURL ?? undefined}
            international={Boolean(data.isInternational)}
          />
        </ul>
        {!(Object.values(paper_json).length === idx + 1) && <div class="divider my-[-1.1rem]" />}
      </>
    ))
  }
</div>
